{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "apps": {
    "ios": {
      "type": "ios.app",
      "build": "xcodebuild -workspace ios/conduitReactNative.xcworkspace -scheme qa -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
      "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/conduitReactNative.app"
    },
    "android.qa.release": {
      "type": "android.apk",
      "binaryPath": "android/app/build/outputs/apk/qa/release/app-qa-release.apk",
      "build": "cd android && ./gradlew assembleQaRelease assembleQaReleaseAndroidTest -DtestBuildType=release && cd .."
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 12"
      }
    },
    "emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "Nexus_One_API_28"
      }
    }
  },
  "configurations": {
    "ios": {
      "device": "simulator",
      "app": "ios"
    },
    "android.emu.qa.release": {
      "device": "emulator",
      "app": "android.qa.release"
    }
  }
}